{
  "name": "@eu-ge-ne/toy",
  "version": "0.0.28",
  "license": "MIT",
  "exports": "./main.ts",
  "workspace": ["app", "ui/*", "lib/*"],
  "compilerOptions": {
    "noUncheckedIndexedAccess": true
  },
  "lint": {
    "rules": {
      "tags": ["recommended"],
      "include": [
        "eqeqeq",
        "explicit-module-boundary-types",
        "explicit-function-return-type"
      ],
      "exclude": ["require-await", "no-control-regex"]
    }
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@^1.0.14",
    "@std/cli": "jsr:@std/cli@^1.0.22",
    "@std/fmt": "jsr:@std/fmt@^1.0.8"
  }
}
