{
  "name": "@eu-ge-ne/toy",
  "version": "0.0.28-rc",
  "license": "MIT",
  "exports": "./main.ts",
  "workspace": ["components/*", "lib/*"],
  "compilerOptions": {
    "noUncheckedIndexedAccess": true
  },
  "lint": {
    "rules": {
      "tags": ["recommended"],
      "include": [
        "eqeqeq",
        "explicit-module-boundary-types",
        "explicit-function-return-type"
      ],
      "exclude": ["require-await", "no-control-regex"]
    }
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@^1.0.18",
    "@std/cli": "jsr:@std/cli@^1.0.27",
    "@std/fmt": "jsr:@std/fmt@^1.0.9"
  }
}
